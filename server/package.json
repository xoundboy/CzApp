{
  "name": "CZappAPI",
  "version": "1.0.0",
  "description": "REST server for CZapp",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'app/**/*.ts' --exec 'ts-node' app/app.ts",
    "serve-prod": "nodemon app.js",
    "build": "tsc --outFile build/app.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/translate": "^2.1.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "google-auth-library": "^3.0.1",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.4"
  }
}
