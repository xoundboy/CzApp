{
	"name": "typescript-express",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "concurrently --kill-others \"tsc -w -p src\" \"node dist/index.js\"",
		"serve": "concurrently --kill-others \"tsc -p src\" \"node dist/index.js\"",
		"debug": "concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect --require ts-node/register src/index.ts\"",
		"test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
		"test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
		"build": "tsc -p src",
		"start": "node dist/index.js",
		"build:docker": "docker build -t rjmacarthy/express-typescript-starter .",
		"docker:compose": "docker-compose up",
		"dumpnodata": "./sh/db.sh dumpnodata",
		"dump": "./sh/db.sh dump",
		"loadnodata": "./sh/db.sh loadnodata",
		"load": "./sh/db.sh load"
	},
	"dependencies": {
		"body-parser": "^1.13.3",
		"cookie-parser": "~1.3.5",
		"cors": "^2.8.5",
		"cross-env": "^5.1.5",
		"debug": "^4.0.1",
		"express": "^4.13.4",
		"fresh": "^0.5.2",
		"glob": "^5.0.14",
		"google-auth-library": "^2.0.2",
		"jade": "~1.11.0",
		"lodash": "^4.17.11",
		"mongoose": "^4.11.1",
		"morgan": "^1.6.1",
		"mysql": "^2.9.0",
		"negotiator": "^0.6.1",
		"serve-favicon": "~2.3.0"
	},
	"devDependencies":
	{
		"@types/mysql": "^2.15.5",
		"@types/chai": "^4.1.3",
		"@types/express": "^4.0.36",
		"@types/lodash": "^4.14.68",
		"@types/mocha": "^5.2.0",
		"@types/mongoose": "^4.7.18",
		"@types/node": "^8.0.9",
		"@types/sinon": "^4.3.3",
		"@types/cors": "^2.8.4",
		"chai": "^4.1.2",
		"concurrently": "^3.5.0",
		"mocha": "^5.1.1",
		"node-mocks-http": "^1.7.0",
		"nodemon": "^1.17.4",
		"sinon": "^5.0.7",
		"ts-node": "^7.0.1",
		"typescript": "^2.4.1"
	}
}
